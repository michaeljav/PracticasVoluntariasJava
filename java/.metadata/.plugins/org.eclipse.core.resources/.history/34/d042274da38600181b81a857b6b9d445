import java.sql.Time;

public class Serie extends Pelicula {


	private int no_temporada;

	
	
	public Serie() {
		
		this.no_temporada =1;
	
	}
	
	
	public Serie(int no_temporada, String genero, Time duracion) {
		super("Creado por defecto",genero, "Michael Javier", no_temporada, duracion,false);
		this.no_temporada = no_temporada;
			
	
	}

	
	public int getNo_temporada() {
		return no_temporada;
	}
	public void setNo_temporada(int no_temporada) {
		
		this.no_temporada = no_temporada;
	}


	@Override
	public String toString() {
		return "Serie [no_temporada=" + no_temporada + ", getTitulo()=" + getTitulo() + ", getGenero()=" + getGenero()
				+ ", getCreador()=" + getCreador() + ", getAño()=" + getAño() + ", getDuracion()=" + getDuracion()
				+ "]";
	}
	
	
}
