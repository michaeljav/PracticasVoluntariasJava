
public class Serie {

	private String titulo;
	private int no_temporada;
	private boolean visto;
	private String genero;
	private String creador;
	private double duracion;
	
	
	public Serie() {
		this.no_temporada =1;
		this.visto=false;
	}
	
	
	public Serie(int no_temporada, String genero, double duracion) {
		super();
		this.no_temporada = no_temporada;
		this.visto = false;
		this.genero = genero;
		this.duracion = duracion;
		this.titulo="Creado por defecto";
		this.creador="Michael Javier";
	}


	public String getTitulo() {
		return titulo;
	}
	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}
	public int getNo_temporada() {
		return no_temporada;
	}
	public void setNo_temporada(int no_temporada) {
		
		this.no_temporada = no_temporada;
	}
	
	public String getGenero() {
		return genero;
	}
	public void setGenero(String genero) {
		this.genero = genero;
	}
	public String getCreador() {
		return creador;
	}
	public void setCreador(String creador) {
		this.creador = creador;
	}
	public double getDuracion() {
		return duracion;
	}
	public void setDuracion(double duracion) {
		this.duracion = duracion;
	}


	@Override
	public String toString() {
		return "Serie [titulo=" + titulo + ", no_temporada=" + no_temporada + ", visto=" + visto + ", genero=" + genero
				+ ", creador=" + creador + ", duracion=" + duracion + "]";
	}
}
